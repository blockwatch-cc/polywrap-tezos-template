{
  "name": "@blocwatch-cc/polywrap-template",
  "description": "Tezos Polywrap Template",
  "private": true,
  "version": "0.0.1-prealpha.77",
  "scripts": {
    "reset": "yarn clean && yarn build",
    "clean": "npx rimraf ./node_modules ./build ./coverage ./.w3 ./**/w3",
    "build": "yarn build:web3api",
    "build:web3api": "npx w3 build",
    "codegen": "npx w3 codegen",
    "test": "yarn test:e2e",
    "test:e2e": "jest --passWithNoTests --runInBand --verbose",
    "test:env:up": "npx w3 test-env up",
    "test:env:down": "npx w3 test-env down",
    "deploy": "yarn deploy:web3api",
    "deploy:web3api": "npx w3 build --ipfs http://localhost:5001 --test-ens harbinger.web3api.eth"
  },
  "devDependencies": {
    "@blockwatch-cc/tezos-plugin-js": "0.0.1-prealpha.80",
    "@blockwatch-cc/tezos-test-env": "0.0.1-prealpha.77",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "@taquito/signer": "12.1.0",
    "@web3api/cli": "0.0.1-prealpha.77",
    "@web3api/client-js": "0.0.1-prealpha.77",
    "@web3api/ens-plugin-js": "0.0.1-prealpha.77",
    "@web3api/ethereum-plugin-js": "0.0.1-prealpha.77",
    "@web3api/ipfs-plugin-js": "0.0.1-prealpha.77",
    "@web3api/test-env-js": "0.0.1-prealpha.77",
    "@web3api/wasm-as": "0.0.1-prealpha.77",
    "assemblyscript": "0.19.1",
    "jest": "27.4.7",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "assemblyscript-json": "^1.1.0"
  }
}
